print("stats module loaded")

def word_count(file_contents):
    num_words = len(file_contents.split()) 
    return num_words 

def char_count(file_contents):
    char_counts = {}
    for char in file_contents.lower():
        if char in char_counts:
            char_counts[char] += 1
        else:
            char_counts[char] = 1
    return char_counts